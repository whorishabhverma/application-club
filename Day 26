1876. Substrings of Size Three with Distinct Characters
class Solution {
public:
    int countGoodSubstrings(string arr) {
        int count=0;
        if(arr.size()>=3){
        for(int i=0;i<arr.size()-2;i++){
            if(arr[i]!=arr[i+1] && arr[i]!=arr[i+2] && arr[i+1]!=arr[i+2])
            count++;

        }
        }
        return count;
        
    }
};
